# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback

# GitHub OAuth Configuration
VITE_GITHUB_CLIENT_ID=your-github-client-id
VITE_GITHUB_REDIRECT_URI=http://localhost:3000/auth/github/callback

# Development Configuration (Security Settings)
# WARNING: These features are disabled by default for security
# VITE_DEV_ENABLED=true                          # Enable development features
# VITE_DEV_AUTO_AUTH=true                       # Auto-authentication (WARNING: bypasses real auth)
# VITE_DEV_SKIP_AUTH=false                      # Skip authentication (WARNING: disables security)
# VITE_DEV_MOCK_API=false                       # Use mock API responses
# VITE_DEV_TOOLS=false                          # Disable developer tools

# Development User Overrides (only used when VITE_DEV_ENABLED=true)
# VITE_DEV_USER_NAME="Developer Name"            # Override user name
# VITE_DEV_USER_EMAIL="dev@example.com"         # Override user email
# VITE_DEV_USER_ROLE="user"                     # Override user role (admin/user/viewer)
# VITE_DEV_USER_ORG="Development Corp"          # Override user organization